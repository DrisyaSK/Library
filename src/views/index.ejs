              <!-- login signup form-->
              <div class="wrapper">
                <div class="title-text">
                  <div class="title login">Login Form</div>
                  <div class="title signup">Signup Form</div>
                </div>
                <div class="form-container">
                  <div class="slide-controls">
                    <input type="radio" name="slide" id="login" checked>
                    <input type="radio" name="slide" id="signup">
                    <label for="login" class="slide login">Login</label>
                    <label for="signup" class="slide signup">Signup</label>
                    <div class="slider-tab"></div>
                  </div>
                  <div class="form-inner">
                    <form action="/login" method="POST" class="login">
                      <div class="field">
                        <input type="text" id="userlgn" placeholder="Username" name="userlgn" required>
                      </div>
                      <div class="field">
                        <input type="password" id="passlgn" placeholder="Password" name="passlgn" required>
                      </div>
                      <div class="pass-link"><a href="#">Forgot password?</a></div>
                    
                      <p id="msg1"></p>
                      <div class="field btn">
                      
                        <div class="btn-layer"></div>
                      
                        <input type="submit" id="btnlgn" value="Login" onclick="check()">
                      </div>
                      <div class="signup-link">Not a member? <a href="">Signup now</a></div>
                    </form>
                    <form action="/signup"  method="POST" class="signup">
                      <div class="field">
                        <input id="username" type="text" placeholder="Username" name="username" required>
                      </div>
                      <div class="field">
                        <input type="password" id="password" placeholder="Password" name="password" required>
                      </div>
                      <div class="field">
                        <input type="password" id="confirm" placeholder="Confirm password" name="confirm" required>
                      </div>
                      <p id="msg1"></p><p id="msg2"></p>
                    
                      <div class="field btn">
                      </form>                 
                      <div class="btn-layer"></div>
                      <input type="submit" value="Signup" id="btn" onclick="validate()">
                      </div>
                      
                  </div>
                </div>
              </div>
                <!--login signup form end-->
